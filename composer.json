{
    "name": "tecnodesignc/core-module",
    "type": "encore-module",
    "description": "The core module for EncoreCMS. This is required for every install",
    "keywords": [
        "encorecms",
        "core"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "marcos21.009@gmail.com"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/Platform/issues",
